запись
обработать их(+,-,*,/,&...)
проверка USERMODE
сброс шин(кроме ab)
считать num64
считать r2
считать r3
считать r3 -> sdb
считать r3_u -> sdb
считать sp
увеличение sp
уменьшение sp
чтение
проверка zero
проверка carry
проверка sign

0 stage:
	проверка USERMODE
	проверка zero
	проверка carry
	проверка sign
	ISINTERRUPT off
	уменьшение sp
	num8 -> core_int

1 stage:
	считать num64
	считать r2
	считать r3
	считать sp

2 stage:
	обработать их(+,-,*,/,&...)

3 stage:
	sdb -> ab

4 stage:
	сброс шин(кроме ab)

5 stage:
	считать r3 -> sdb
	считать r3_u -> sdb
	считать pc -> sdb

тут будет pc += размер инструкции

6 stage:
	запись
	чтение
	увеличение sp

	flag -> sdb
	state -> sdb
	tp -> sdb

	r3 -> pc

7 stage:
	sdb -> r1
	sdb -> r1_u
	sdb -> flag
	sdb -> state
	sdb -> tp
